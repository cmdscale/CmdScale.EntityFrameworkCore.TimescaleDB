{
  "stryker-config": {
    "project-info": {
      "name": "Eftdb",
      "module": "Eftdb.Tests"
    },
    "solution": "../../CmdScale.EntityFrameworkCore.TimescaleDB.sln",
    "project": "Eftdb.csproj",
    "target-framework": "net10.0",
    "concurrency": 8,
    "mutation-level": "Standard",
    "language-version": "latest",
    "additional-timeout": 10000,
    "mutate": [
      "**/*.cs",
      "!**/*.Designer.cs",
      "!**/obj/**/*.cs",
      "!**/bin/**/*.cs",
      "!**/Migrations/**/*.cs",
      "!**/Properties/**/*.cs"
    ],
    "coverage-analysis": "perTest",
    "disable-bail": true,
    "disable-mix-mutants": false,
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 0
    },
    "reporters": [
      "html",
      "progress",
      "cleartext"
    ],
    "report-file-name": "mutation-report",
    "since": {
      "enabled": true,
      "target": "main",
      "ignore-changes-in": [
        "**/samples/**",
        "**/benchmarks/**",
        "**/Eftdb.FunctionalTests/**"
      ]
    },
    "break-on-initial-test-failure": true
  }
}
